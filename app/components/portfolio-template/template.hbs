<div class="container no-content-background">
  <div class="row">
    <div class="col-xs-12">
      <h1>Gallery</h1>
      <div class="gallery">
        {{#each @groups as |group|}}
          {{#link-to "group" group.id class="pic"}}
            <h4>{{group.title}}</h4>
            <img src={{group.image}} />

          {{/link-to}}
        {{/each}}
      </div>
    </div>
  </div>
</div>
